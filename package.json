{
  "name": "wyx-utils",
  "arthor": "Yuxi Wang",
  "version": "0.2.6-0",
  "description": "A place to put commonly used utilities",
  "main": "dist/cjs/index",
  "module": "dist/esm/index",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Yxwww/wyx-utils.git"
  },
  "author": "Yxwww <yuxi.wangcd@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint src",
    "test": "mocha --require sucrase/register --recursive src --extension ts",
    "test:watch": "mocha --require sucrase/register --recursive src/**/*.test.ts src/**/*.test.js --watch ",
    "coverage": "c8 npm run test",
    "prepare": "npm run build:typing && npm run build:esm && npm run build:cjs",
    "build:typing": "tsc --emitDeclarationOnly",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "c8": "^7.10.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "sucrase": "^3.20.3",
    "typescript": "^4.5.4"
  },
  "bugs": {
    "url": "https://github.com/Yxwww/wyx-utils/issues"
  },
  "homepage": "https://github.com/Yxwww/wyx-utils#readme",
  "keywords": [
    "utils"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --cache --fix"
  },
  "dependencies": {
    "D": "^1.0.0"
  }
}
