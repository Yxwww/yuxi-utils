{
  "name": "yuxi-utils",
  "arthor": "Yuxi Wang",
  "version": "0.3.0",
  "description": "A place to put commonly used utilities",
  "main": "dist/cjs/index",
  "module": "dist/esm/index",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Yxwww/wyx-utils.git"
  },
  "author": "Yxwww <yuxi.wangcd@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "pnpm build:typing && pnpm build:esm && pnpm build:cjs",
    "build:typing": "tsc --emitDeclarationOnly",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "eslint": "^8.57.1",
    "husky": "^9.1.6",
    "mocha": "^9.2.2",
    "prettier": "^2.8.8",
    "sucrase": "^3.35.0",
    "typescript": "^4.9.5",
    "vitest": "^3.0.8",
    "yuxi-scripts": "^0.0.6"
  },
  "bugs": {
    "url": "https://github.com/Yxwww/wyx-utils/issues"
  },
  "homepage": "https://github.com/Yxwww/wyx-utils#readme",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "keywords": [
    "utils"
  ]
}
